{
  "package": {
    "productName": "ServerMilk",
    "version": "1.2.4"
  },
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "fs": {
        "scope": [
          "**"
        ],
        "createDir": true,
        "readDir": true,
        "writeFile": true
      },
      "dialog": {
        "all": true,
        "open": true,
        "save": true
      },
      "shell": {
        "sidecar": true,
        "scope": [
          {
            "name": "binaries/serverbee-web",
            "sidecar": true,
            "args": [
              "-p",
              {
                "validator": "\\S+"
              },
              "-l",
              {
                "validator": "\\S+"
              }
            ]
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [
        "binaries/serverbee-web"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "app.serverbee",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://serverbee-1253263310.cos.ap-shanghai.myqcloud.com/desktop/updater.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDM2RDU5RTkwQkI1NzFENDQKUldSRUhWZTdrSjdWTnMxVEg1Yjh5eCt1M2JCNytIdVNuN3duVm9LcUoveXdyTmlnU21ZTFEyejUK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 360,
        "resizable": true,
        "title": "ServerMilk",
        "width": 320,
        "titleBarStyle": "Transparent"
      }
    ],
    "systemTray": {
      "iconPath": "icons/mac-icon.ico",
      "iconAsTemplate": true
    }
  }
}
